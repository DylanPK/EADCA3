@page "/MapGame"
@inject IJSRuntime JSRuntime

<PageTitle>Map Game</PageTitle>

<h1>Map Game</h1>


@code{
    protected override async Task OnAfterRenderAsync(bool firstRender)
    {
        if (firstRender)
        {
            await JSRuntime.InvokeVoidAsync("initMap", null); 
        }
    }
}
